{
	"useTabs": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "none",
	"semi": true,
	"printWidth": 100,
	"plugins": ["prettier-plugin-tailwindcss",  "prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
